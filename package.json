{
  "name": "jobibox",
  "version": "1.4.2",
  "description": "La cabine accoustique pour créer un cv vidéo",
  "repository": "https://github.com/EvilSpartans/JobiBox",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "watch": "webpack --config webpack.common.js --watch",
    "build": "webpack --config webpack.prod.js",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "make:linux": "electron-builder --linux",
    "make:macos": "electron-builder --mac",
    "make:win": "electron-builder --win --x64 --publish always"
  },
  "author": "Jobissim",
  "license": "ISC",
  "permissions": [
    {
      "electron": true,
      "devices": [
        "videoCapture",
        "microphone"
      ]
    }
  ],
  "build": {
    "productName": "jobibox",
    "appId": "com.jobibox",
    "publish": {
      "provider": "github",
      "owner": "EvilSpartans",
      "repo": "JobiBox"
    },
    "files": [
      "build/**/*",
      "assets/**/*",
      "utils/**/*",
      "index.html",
      "splash.html",
      "splash.css",
      "main.js",
      "preload.js"
    ],
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/images/logomac.png",
      "hardenedRuntime": true,
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 430,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "publish": {
        "provider": "github",
        "owner": "EvilSpartans",
        "repo": "JobiBox"
      },
      "icon": "assets/images/logowin.png",
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Development"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.22.17",
    "dotenv-webpack": "^8.0.1",
    "electron": "^26.2.1",
    "electron-builder": "^24.6.4",
    "file-loader": "^6.2.0",
    "tailwindcss": "^3.3.3",
    "webpack-merge": "^5.9.0"
  },
  "dependencies": {
    "@babel/preset-env": "^7.22.15",
    "@babel/preset-react": "^7.22.15",
    "@fortawesome/fontawesome-svg-core": "^6.4.2",
    "@fortawesome/free-solid-svg-icons": "^6.4.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@hookform/resolvers": "^3.3.1",
    "@mediapipe/selfie_segmentation": "^0.1.1675465747",
    "@reduxjs/toolkit": "^1.9.6",
    "@uiw/react-color-wheel": "^2.0.6",
    "axios": "^1.5.1",
    "babel-loader": "^9.1.3",
    "css-loader": "^6.8.1",
    "electron-localshortcut": "^3.2.1",
    "electron-reload": "^2.0.0-alpha.1",
    "electron-updater": "^4.6.5",
    "express": "^4.18.2",
    "intro.js-react": "^1.0.0",
    "pako": "^1.0.11",
    "postcss-loader": "^7.3.3",
    "rc-select": "^14.13.1",
    "rc-slider": "^10.4.0",
    "react": "^18.2.0",
    "react-datepicker": "^4.21.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-hook-form": "^7.47.0",
    "react-media-recorder": "^1.6.6",
    "react-modal": "^3.16.1",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.16.0",
    "react-select": "^5.7.7",
    "react-slick": "^0.29.0",
    "react-slider": "^2.0.6",
    "react-spinners": "^0.13.8",
    "react-webcam": "^7.2.0",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.22",
    "sass": "^1.66.1",
    "sass-loader": "^13.3.2",
    "slick-carousel": "^1.8.1",
    "style-loader": "^3.3.3",
    "tw-elements-react": "^1.0.0-alpha1",
    "video-react": "^0.16.0",
    "webpack": "^5.88.2",
    "webpack-cli": "^5.1.4",
    "yup": "^1.3.2",
    "zlibjs": "^0.3.1"
  }
}
